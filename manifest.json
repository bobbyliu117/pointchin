{
  "name": "PointChin",
  "description": "Calculate ROI for you",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js":["dist/content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "static/popup.html",
    "default_title": "Calculate ROI"
  },
  "background": {
    "scripts": ["dist/background.js"]
  },
  "permissions": ["tabs"],
  "icons": { "16": "static/calculator-16.png", "128": "static/calculator-128.png" }
}